 pipeline{
 agent any
	// agent {docker { image 'node:13.8'}}
stages{
	stage('Build'){
		steps {
			echo "hi"
			echo "node version is"
			//sh 'node --version'
			echo "BUILD_NUMBER"
			echo "$env.BUILD_NUMBER"
			echo "BUILD_TAG"
			echo "$env.BUILD_TAG"
			echo "this is declarative pipiline"
			}
	}
}
}
